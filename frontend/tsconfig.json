{
  "compilerOptions": {
    "esModuleInterop": true,
    "lib": [
      "es2020",
      "dom"
    ],
    "module": "es2022",
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "strict": true,
    "sourceMap": true,
    "target": "es2022",
    "types": [],
    "outDir": "dist",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@ui/*": [
        "./src/components/ui/*"
      ],
      "@api/*": [
        "./src/api/*"
      ]
    }
  }
}